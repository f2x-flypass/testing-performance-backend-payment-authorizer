          <boolProp name="resetInterpreter">false</boolProp>
          <stringProp name="script">// Definir el tiempo objetivo entre muestras en milisegundos (por ejemplo, 300000 ms para 5 minutos)
long targetInterval =4000;
long lastSampleTime = vars.getObject(&quot;lastSampleTime&quot;) != null ? (Long) vars.getObject(&quot;lastSampleTime&quot;) : 0;
long currentTime = System.currentTimeMillis();

// Si el tiempo desde la última muestra es menor que el intervalo objetivo, dormir
if (currentTime - lastSampleTime &lt; targetInterval) {
    long sleepTime = targetInterval - (currentTime - lastSampleTime);
    Thread.sleep(sleepTime);
}

vars.putObject(&quot;lastSampleTime&quot;, currentTime);
</stringProp>
