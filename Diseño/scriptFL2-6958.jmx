          <stringProp name="script">import java.security.MessageDigest

// Generar un string aleatorio
def randomString = UUID.randomUUID().toString().replaceAll(&quot;-&quot;, &quot;&quot;)

// Crear un hash MD5 para ajustarse al formato de 32 caracteres hexadecimales
MessageDigest md = MessageDigest.getInstance(&quot;MD5&quot;)
md.update(randomString.getBytes())
byte[] digest = md.digest()

// Convertir a formato hexadecimal
String traceId = digest.collect { String.format(&quot;%02x&quot;, it) }.join(&quot;&quot;)

// Almacenar el Trace-ID en una variable para usarlo en el test
vars.put(&quot;trace_id&quot;, traceId)
